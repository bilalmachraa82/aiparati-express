{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://aiparati-express-backend.vercel.app"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://aiparati-express-backend.vercel.app/api/$1"
    }
  ]
}